# configs.Config

# configs.Server
server:
  debug: true
  addr: localhost
  port: 8000
  read_timeout: 60
  max_conn: 1000

# configs.JWT
jwt:
  secret: meeseeks
  expire: 3600

# configs.Database
database:
  mode: 1
  driver: mysql
  prefix: m_
  charset: utf8mb4
  max_idle_conn: 100
  max_open_conn: 100
  conn_max_lifetime: 60
  source: meeseeks:meeseeks@tcp(localhost:13306)/meeseeks?charset=utf8mb4&parseTime=True&loc=Local
  log_path: ./log/sql.log


webhook:
  feishu:
    app_id: # 应用id
    secret: # 应用秘钥
    encrypt_key:  # webhook key 可选
    verification_token:  # webhook token 可选

